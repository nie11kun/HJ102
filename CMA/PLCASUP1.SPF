PROC PLCASUP1 DISPLOF
;***********程序功能**********

G94

DRFOF

IF $A_DBB[2]<>0;异常退刀
    G90 G01 Z=INI[167] F=INI[56]
    G90 G01 X=INI[23] F=INI[54]

    GOTOF ABORT_DELAY
ENDIF

IF $A_DBB[15]<>0;修整位退刀
    G90 G01 Z=INI[167] F=INI[56]
    G90 G01 X=INI[10] F=INI[54]

    GOTOF ABORT_DELAY
ENDIF

ABORT_DELAY:
MSG("请处理...")
G4 F999999
GOTOB ABORT_DELAY

M17

